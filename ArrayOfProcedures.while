let
	array[3]: 0;
	proc p1() do print toString("Procedure 1") od;
	proc p2() do print toString("Procedure 2") od;
	proc p3() do print toString("Procedure 3") od;

	proc loop(array)
		let i: 0; p: 0
		in
			while <(!i,array.length)
			do
				try do
					p := array[!i];
					i := +(!i,1);
					call p() od
				catch e
					print toString(!e)
				yrt
			od
		end
in
	array[0] := p1;
	array[1] := p2;
	array[2] := p3;

	call loop(array)
end